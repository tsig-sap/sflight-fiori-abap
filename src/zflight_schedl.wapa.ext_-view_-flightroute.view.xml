<mvc:View                                                                                                                                                                                                                                                      
	xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                                   
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:html="http://www.w3.org/1999/xhtml"                                                                                                                                                                                                                     
	xmlns:vbm="sap.ui.vbm"                                                                                                                                                                                                                                        
	xmlns:l="sap.ui.layout"                                                                                                                                                                                                                                       
	height="100%"                                                                                                                                                                                                                                                 
	displayBlock="true"                                                                                                                                                                                                                                           
	controllerName="sflight.flightschedule.ext.controller.FlightRoute">                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
	<vbm:AnalyticMap id="_IDGenAnalyticMap1">                                                                                                                                                                                                                     
		<vbm:vos>                                                                                                                                                                                                                                                    
			<vbm:Routes id="_IDGenRoutes1">                                                                                                                                                                                                                             
				<vbm:Route id="_IDGenRoute1"                                                                                                                                                                                                                               
					position="{ToLongitude};{ToLatitude};0;{FromLongitude};{FromLatitude};0"                                                                                                                                                                                  
					color="rgb(117,148,34)"                                                                                                                                                                                                                                   
					colorBorder="rgb(255,255,255)"                                                                                                                                                                                                                            
					linewidth="3"                                                                                                                                                                                                                                             
					start="1"                                                                                                                                                                                                                                                 
					routetype="Geodesic" />                                                                                                                                                                                                                                   
			</vbm:Routes>                                                                                                                                                                                                                                               
		</vbm:vos>                                                                                                                                                                                                                                                   
	</vbm:AnalyticMap>                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
</mvc:View>                                                                                                                                                                                                                                                    